{
  "name": "inventorization-asset-management",
  "version": "1.0.0",
  "description": "Simultaneously Run the App",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "npm start --prefix api",
    "client": "npm start --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "install": "cd api && npm install && cd ../client && npm install"
  },
  "repository": {
    "type": "git"
  },
  "author": "TM",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
